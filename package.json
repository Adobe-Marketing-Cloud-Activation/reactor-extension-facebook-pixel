{
  "name": "extension-facebookpixel",
  "version": "0.0.1",
  "description": "Facebook Pixel extension for Composer",
  "author": {
    "name": "DTM",
    "email": "DTM-Engineering@adobe.com"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Adobe-Marketing-Cloud-Activation/extension-facebookpixel.git"
  },
  "publishConfig": {
    "registry": "https://artifactory.corp.adobe.com/artifactory/api/npm/npm-mcps-release-local"
  },
  "keywords": [
    "turbine",
    "extension",
    "facebook pixel"
  ],
  "scripts": {
    "build": "webpack --production",
    "lint": "reactor-testrunner lint",
    "package": "npm run build && composer-packager",
    "sandbox": "webpack --watch --run-sandbox",
    "bootstrap": "composer-sandbox init",
    "test": "reactor-testrunner",
    "ci-job": "xvfb-run npm test -- --ci --browser chrome firefox ie9sg ie10sg ie11sg edgesg && npm run lint && npm run package"
  },
  "devDependencies": {
    "@adobe/composer-packager": "^2.3.3",
    "@adobe/composer-sandbox": "^7.8.5",
    "@adobe/composer-turbine": "^18.0.0",
    "@reactor/extension-support-testrunner": "^9.0.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "html-webpack-externals-plugin": "2.1.2",
    "html-webpack-plugin": "^2.24.1",
    "react-addons-test-utils": "^15.3.2",
    "webpack-shell-plugin": "^0.4.3",
    "yargs": "^6.3.0"
  },
  "dependencies": {
    "@coralui/react-coral": "<1.0.0",
    "@coralui/redux-form-react-coral": "<1.0.0",
    "@reactor/react-components": "^3.1.1",
    "css-loader": "^0.25.0",
    "deep-extend": "^0.4.1",
    "deep-get-set": "^1.0.0",
    "history": "^4.4.0",
    "import-glob-loader": "^1.1.0",
    "nib": "^1.1.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "reduce-reducers": "^0.1.2",
    "redux": "^3.6.0",
    "redux-actions": "^0.13.0",
    "redux-form": "6.2.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "tiny-emitter": "^1.1.0",
    "uuid-v4": "^0.1.0"
  }
}
